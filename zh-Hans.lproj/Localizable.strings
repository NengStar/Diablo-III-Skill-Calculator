"rune_flamingdart_name" = "Flaming Dart";
"rune_flamingdart_detail" = "解锁于等级 43\nIgnite the dart so that it deals 160% weapon damage as Fire at once.";

"rune_snaketotheface_name" = "Snake to the Face";
"rune_snaketotheface_detail" = "解锁于等级 52\nTransform your Poison Dart into a snake that has a 30% chance to Stun the enemy for 1.5 seconds.";




"rune_unbreakablegrasp_name" = "Unbreakable Grasp";
"rune_unbreakablegrasp_detail" = "解锁于等级 7\nIncreases the Slow amount to 80%.";

"rune_gropingeels_name" = "Groping Eels";
"rune_gropingeels_detail" = "解锁于等级 15\nIncreases the damage done to 26% weapon damage as Physical.";

"rune_deathislife_name" = "Death Is Life";
"rune_deathislife_detail" = "解锁于等级 25\nEnemies who die while in the area of Grasp of the Dead have a 5% chance to produce a health globe.";

"rune_desperategrasp_name" = "Desperate Grasp";
